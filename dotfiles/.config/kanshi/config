profile {
    output eDP-1 enable mode 1920x1080@60Hz
}

profile {
    output HDMI-A-1 enable mode 2560x1440@75Hz
    output eDP-1 disable
}

# Dual monitor setup (Profile: Dual Monitors)
profile {
    output DP-2 enable mode 1920x1080@165Hz position 0,0
    output HDMI-A-1 enable mode 1920x1080@60Hz position 1920,0
}

