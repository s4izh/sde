Host github.com
  User git
  HostName github.com
  IdentityFile  ~/.ssh/github

Host sistemes
  Port 22
  ForwardX11 yes
  ForwardX11Trusted yes
  User alumne
  HostName 192.168.122.10
  IdentityFile ~/.ssh/sistemes

Host *
  ForwardAgent no
  Compression no
  ServerAliveInterval 0
  ServerAliveCountMax 3
  HashKnownHosts no
  UserKnownHostsFile ~/.ssh/known_hosts
  ControlMaster no
  ControlPath ~/.ssh/master-%r@%n:%p
  ControlPersist no
