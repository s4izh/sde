super + w
    firefox

super + u: {k,m,s,l,r,c,u}
	{dmenukblayout, dmenumonitor, dmenusys, dmenuopgg, dmenuraco, dmenuconfig, dmenuopenuni}

super + r
    dmenuremote

super + shift + t
    alacritty -e themeselector

super + g
    alacritty -e repo

super + v
    alacritty --class volumen,volumen -e pulsemixer

super + shift + i
    alacritty --class inbox,inbox -e nvim ~/notes/index.md

XF86AudioRaiseVolume
	amixer set Master 5%+

XF86AudioLowerVolume
    amixer set Master 5%-

XF86AudioMute
	amixer set Master toggle

super + shift + s
	screenshot

Print
	screenshot --name-prompt

XF86MonBrightnessUp
	brightnessctl set +10%

XF86MonBrightnessDown
	brightnessctl set 10%-

super + n
    nixsearch

super + ctrl + n
	redshift -O 5000

super + shift + n
	redshift -x

super + alt + s
    feh --bg-fill $(find ~/pix/wall -type f | shuf -n 1)

super + e
    launch_emacs

super + shift + e
    killall emacs && twmnc -t emacs -c "killed all emacs instances  "
